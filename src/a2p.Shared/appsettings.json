{
    "AppSettings": {
        "Folders": {
            "RootFolder": "C:/Temp/Import",
            "ImportSuccess": "Import_Success",
            "ImportFailed": "Import_Failed",
            "Log": "Log"
        },
        "Model": {
            "Sapa": "Sapa_ALU",
            "SapaV2": "Sapa_ALU",
            "Schueco": "Schueco_ALU"
        },
        "RefreshFilesOnStartup": "false",
        "Staging": "true",
        "LogLevelFilter": [
            "Verbose",
            "Debug",
            "Information",
            "Warning",
            "Error",
            "Fatal"
        ]

    },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=PHOBOS;Integrated Security=True;Initial Catalog=PrefSuite_Nordan_BC2;Connect Timeout=30;Encrypt=False;Trust Server Certificate=True;"
  },

    "Serilog": {
        "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.EventLog", "Serilog.Sinks.File", "Serilog.Exceptions", "Serilog" ],

      "MinimumLevel": {
        "Default": "Warning",
        "Override": {
          "Microsoft": "Warning",
          "Microsoft.Hosting.Lifetime": "Information"
        }
      },
        "Enrich": [
            "WithExceptionDetails"
        ],
        "WriteTo": [
            {
                "Name": "Console",
                "Args": {
                    "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level}] {Message} {Properties} {NewLine}{Exception}"
                }
            },
            {
                "Name": "EventLog",
                "Args": {
                    "source": "Uniwave",
                    "logName": "Any2Prefsuite",
                    "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level}] {Message} {Properties} {NewLine}{Exception}"
                }
            },
            {
                "Name": "File",
                "Args": {
                }
            },
            {
                "Name": "Debug",
                "Args": {
                }
            }
        ]

    }
}